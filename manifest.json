{
  "manifest_version": 2,
  "name": "Imgblock",
  "version": "0.1.0",
  "description": "Think Flashblock but for images",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["contentstyles.css"],
      "js": ["contentscript.js"]
    }
  ],
  "permissions": []
}
